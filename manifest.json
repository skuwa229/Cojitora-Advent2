{
	"manifest_version": 2,
	"name": "こじとらくん版新しいタブ",
	"description": "新しいタブがこじとらくん仕様になるよ。",
	"version": "1.0",
	"icons": {
		"16": "image/icon16.png",
		"48": "image/icon48.png",
		"128": "image/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"48": "image/icon48.png"
		},
		"default_title": "新しいタブがこじとらくん仕様になるよ。"
	},	
	"chrome_url_overrides": {
			"newtab": "html/newtab.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}

